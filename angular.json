            "main": "apps/mobile/src/main.ts",
            "polyfills": "apps/mobile/src/polyfills.ts",
            "tsConfig": "apps/mobile/tsconfig.app.json",
            "aot": true,
            "progress": false,
            "assets": ["apps/mobile/src/favicon.ico", "apps/mobile/src/assets"],
            "styles": ["apps/mobile/src/styles.scss"],
            "scripts": [],
